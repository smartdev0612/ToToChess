.cbp-af-header1 {
	position: fixed;
	background-position:center;
	top: 100px;
	right:50%;margin-right:-610px;float:right;
	width:750px;
	z-index: 6;
	height: 30px;
	-webkit-transition: height 0.4s;
	-moz-transition: height 0.4s;
	transition: height 0.4s;
}
.cbp-af-header {
	position: fixed;
	background-position:center;
	top: 0;
	left: 0;
	width: 100%;
	min-width:1024px;
	background-color: #09202B;
	background-image:url(/images/ibet/top_bg.jpg);
	z-index: 100000000;
	height: 100px;
	-webkit-transition: height 0.4s;
	-moz-transition: height 0.4s;
	transition: height 0.4s;
}

.cbp-af-header .cbp-af-inner {
	width: 90%;
	max-width: 69em;
	margin: 0 auto;
	padding: 0 1.875em;
}
.cbp-af-header h1,
.cbp-af-header nav {
	position:absolute;
	display: inline-block;
	z-index:40;
}
.cbp-af-header nav {
	margin-top:100px;
	margin-left:170px;

}

 /* We just have one-lined elements, so we'll center the elements with the line-height set to the height of the header */
.cbp-af-header h1,
.cbp-af-header nav ul{
	line-height: 100px; 
}
.cbp-af-header h1 {
	text-transform: uppercase;
	*margin: 43px 0 0 -70px; /* <- ->  logo */
	margin: 0 0 0 -70px; /* <- ->  logo */
	padding-top:-30px;
	float: left;
	font-family:Ubuntu, sans-serif;
	color:#fff;
	font-size:4em;
	font-weight:700;
}
.cbp-af-header h1 img {
	height: 87px;  /* <- ->  logo size */
	-webkit-transition: all 0.4s;
	-moz-transition: all 0.4s;
	transition: all 0.4s;
}
.cbp-af-header nav {
	float: right;
	-webkit-transition: all 0.4s;
	-moz-transition: all 0.4s;
	transition: all 0.4s;
}
.cbp-af-header nav a {
	color: #fff;
	margin: 0 0 0 25px;
	text-align:center;
	display:inline-block;
	letter-spacing:1px;
	cursor:pointer;
}
.cbp-af-header nav a:hover {
	color: #8eccff;
	background-position-y:40px; /* 마우스 오버시 메뉴 한글로 전환 하는거 */
}

/* Transitions and class for reduced height */
.menu_fixed_top,
.cbp-af-header1,
.mypage_btn  {
	-webkit-transition: all 0.4s;
	-moz-transition: all 0.4s;
	transition: all 0.4s;
}
.cbp-af-header1.cbp-af-header-shrink { /* 1280이하 상단 좁을때 */
	top: 67px;
}

.mypage_btn.cbp-af-header-shrink {
	padding-top: 20px;
}


.menu_fixed_top {
	top: 155px;
}
.menu_fixed_top.cbp-af-header-shrink {
	top: 110px;
}

.cbp-af-header h1,
.cbp-af-header nav a {
	-webkit-transition: all 0.4s;
	-moz-transition: all 0.4s;
	transition: all 0.4s;
}
.cbp-af-header.cbp-af-header-shrink {
	height: 95px;   /* -> <-  topbar height */
}
.cbp-af-header.cbp-af-header-shrink nav {
	margin-top:20px;
}
.cbp-af-header.cbp-af-header-shrink nav a {
	display:inline-block;
}
.cbp-af-header.cbp-af-header-shrink h1 {
	line-height: 118px;
}
.cbp-af-header.cbp-af-header-shrink h1 {
	font-size: 3em;
}
.cbp-af-header.cbp-af-header-shrink h1,
.cbp-af-header.cbp-af-header-shrink h1 img {
	font-size: 3em;
	height: 60px;
	padding-top:1px; /* up <- -> logo top */

}



/* Example Media Queries */

@media screen and (max-width: 1650px) {
	
	.cbp-af-header .cbp-af-inner {
		width: 1280px;
	}
	.cbp-af-header h1 {
		display: block;
		margin-top:-15px; /* -> <-  logo */
		text-align: center;
		float: none;
		margin-left:430px;
		line-height: 0px;
	}
	.cbp-af-header nav {
		display: block;
		line-height: 0px;
		text-align: center;
		float: none;
		margin-top:125px;
		margin-left:0px; /* up */
	}
	.cbp-af-header h1,
	.cbp-af-header h1 img,
	.cbp-af-header nav a {
		-webkit-transition: all 0.4s;
		-moz-transition: all 0.4s;
		transition: all 0.4s;
		line-height: 0px;
	}
	
	.cbp-af-header div > img {
		position:absolute;
		z-index:30000;
		margin-top:-24px;
		margin-left:409px;
		-webkit-transition: all 0.4s;
		-moz-transition: all 0.4s;
		transition: all 0.4s;
	}

	.cbp-af-header1 {  /* 1280이하 상단 넓을때 */
		top: 170px;
	}

	.cbp-af-header.cbp-af-header-shrink nav{
		margin-top:20px;  /* up -> <- menu */
	}
	.cbp-af-header.cbp-af-header-shrink h1,
	.cbp-af-header.cbp-af-header-shrink h1 img {
		height: 60px;
		margin-top:-100px; /* up -> <- logo */
		margin-left:240px;
	}
	
	.cbp-af-header.cbp-af-header-shrink div > img {
		position:absolute;
		z-index:30000;
		margin-top:-221px;
		margin-left:500px;
		-webkit-transition: all 0.4s;
		-moz-transition: all 0.4s;
		transition: all 0.4s;
	}
		
	.submenu_1 {
		text-align:left;
	}
	.submenu_2 {
		/*margin-left:870px;*/
		text-align:left;
	}
	.submenu_2.scroll {
		margin-top:0px;
	}
}

