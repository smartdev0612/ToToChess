.submenu_1{
	margin:0px auto;
	padding:0px;
	position:fixed;
	width:225px;
	background-color:#174F6C;
	line-height:30px;
	-webkit-transition: all 0.4s;
	-moz-transition: all 0.4s;
	transition: all 0.4s;
	z-index:30;
	margin-top:0px;
	display:none;
}
.submenu_1 li{
	border-bottom: 1px solid #10364B;
	border-top: 1px solid #206C95;
	line-height:40px;
	font-size:0.9em;
	padding-left:15px;
	padding-bottom:3px;
	color:rgba(255, 255, 255, 1);
	cursor:pointer;
	text-align:left;
}
